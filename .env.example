# ============================================================================
# Benchmark Configuration
# ============================================================================
# Copy this file to .env and adjust values as needed

# Total number of tax codes to generate
TOTAL_IDS=15000000

# Batch sizes for different operations
BATCH_SIZE_COMPUTATION=100000
BATCH_SIZE_CF_RAW=50000
BATCH_SIZE_KEYDB=50000
BATCH_SIZE_POSTGRES=50000
BATCH_SIZE_POSTGRES_UPDATE=50000
MEGA_BATCH_SIZE=2000000

# Benchmark execution flags (true/false)
RUN_POSTGRES_CF_WRITE=true
RUN_KEYDB=true
RUN_POSTGRES_INSERT=true
RUN_POSTGRES_SALT_UPDATE=false

# ============================================================================
# Security Configuration
# ============================================================================
# Salt for SHA256 hashing
CF_HASH_SALT=CF_ANPR_2025_SALT_KEY
# New salt for BENCHMARK 4 (salt rotation scenario)
NEW_SALT=CF_ANPR_2026_NEW_SALT

# ============================================================================
# KeyDB Configuration
# ============================================================================
KEYDB_HOST=localhost
KEYDB_PORT=6379
KEYDB_DB=0

# ============================================================================
# PostgreSQL Configuration
# ============================================================================
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=cf_benchmark
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
